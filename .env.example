# Discord Self-Bot Token
# Get this from your browser's developer tools while logged into Discord
# Instructions: Open Discord in browser > DevTools > Application > Local Storage > token
DISCORD_TOKEN=your_discord_token_here

# Cryptocurrency Private Keys (USE DEDICATED WALLETS WITH LIMITED FUNDS!)
# These keys will be used to sign and send transactions automatically

# Litecoin Private Key (WIF format)
LTC_PRIVATE_KEY=your_ltc_private_key_here

# Solana Private Key (Base58 encoded)
SOL_PRIVATE_KEY=your_solana_private_key_here

# Bitcoin Private Key (WIF format)
BTC_PRIVATE_KEY=your_btc_private_key_here

# Returns/Payout Addresses (Where you receive money)
LTC_PAYOUT_ADDRESS=your_ltc_address_here
SOL_PAYOUT_ADDRESS=your_sol_address_here
BTC_PAYOUT_ADDRESS=your_btc_address_here

# Bot Configuration
# Comma-separated list of user IDs who are trusted middlemen
MIDDLEMAN_IDS=123456789012345678,987654321098765432

# Channel to post win vouches
VOUCH_CHANNEL_ID=123456789012345678

# Webhook for logging games/payments (Optional)
WEBHOOK_URL=https://discord.com/api/webhooks/...
